{
  "jsc": {
    "target": "es2020",
    "baseUrl": ".",
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true
    },
    "keepClassNames": true,
    "loose": false
  },
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false,
    "resolveExtensions": [".ts", ".tsx", ".js", ".jsx"]
  },
  "minify": false,
  "sourceMaps": false,
  "inlineSourcesContent": false
}