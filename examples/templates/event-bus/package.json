{
  "name": "my-event-bus-project",
  "version": "1.0.0",
  "description": "Claude Flow Novice event-driven system (10,000+ events/sec)",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npx claude-flow-novice",
    "eventbus:init": "npx claude-flow-novice eventbus init --throughput-target 10000",
    "eventbus:publish": "npx claude-flow-novice eventbus publish",
    "eventbus:subscribe": "npx claude-flow-novice eventbus subscribe",
    "eventbus:monitor": "npx claude-flow-novice eventbus monitor",
    "eventbus:metrics": "npx claude-flow-novice eventbus metrics",
    "swarm": "node tests/manual/test-swarm-direct.js"
  },
  "dependencies": {
    "claude-flow-novice": "^1.6.6"
  },
  "keywords": [
    "claude",
    "event-bus",
    "event-driven",
    "high-throughput",
    "ai-agents"
  ],
  "author": "",
  "license": "MIT"
}
