# Docker ignore file for Claude Flow Novice
# Excludes unnecessary files from Docker build context

# Git
.git
.gitignore
.gitattributes

# Dependencies that will be installed in container
node_modules
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Large directories to exclude
archive/
.build-backups/
.build-logs/
monitor/

# Development files
.vscode
.idea
*.swp
*.swo
*~

# Test files and coverage
tests/
coverage/
.nyc_output/
test-results/
*.test.js
*.test.ts
*.spec.js
*.spec.ts

# Build artifacts that will be created in container
dist/
build/
.claude-flow-novice/dist/

# Documentation (keep essentials)
docs/
*.md
!README.md
!CLAUDE.md
!LICENSE

# Configuration files (keep essential ones)
.env
.env.local
.env.*.local
config/local/
config/development/

# Logs and temporary files
logs/
*.log
tmp/
temp/
.tmp/

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# IDE and editor files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Cache and data directories
.cache/
.parcel-cache/
.eslintcache
.stylelintcache

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# nyc test coverage
.nyc_output

# Dependency directories
jspm_packages/

# Optional npm cache directory
.npm

# Optional REPL history
.node_repl_history

